{
  "name": "ai-solutions-consulting",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build && cpx public/_redirects dist && cpx public/404.html dist",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "cpx": "^1.2.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^6.3.5"
  },
  "build": {
    "command": "npm run build",
    "publish": "dist"
  },
  "jobs": {
    "build-deploy": {
      "runs-on": "ubuntu-latest",
      "steps": [
        {
          "uses": "actions/checkout@v3"
        },
        {
          "name": "Install dependencies",
          "run": "npm ci"
        },
        {
          "name": "Build",
          "run": "npm run build"
        },
        {
          "name": "Deploy",
          "run": "echo \"Your deploy command here\""
        }
      ]
    }
  }
}
