{
  "name": "ai-solutions-consulting",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "astro build",
    "astro:dev": "astro dev",
    "astro:build": "astro build",
    "test": "echo \"No tests specified\" && exit 0",
    "fetch-posts": "node scripts/fetch-news.js",
    "test-fetch": "node scripts/test-fetch.js",
    "create-mock-posts": "node scripts/create-mock-posts.js",
    "dev": "astro dev",
    "preview": "astro preview",
    "daily-update": "npm run fetch-posts && npm run build"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.9.0",
    "framer-motion": "^12.18.1",
    "gray-matter": "^4.0.3",
    "next-mdx-remote": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "astro": "^5.10.2",
    "autoprefixer": "^10.4.0",
    "cpx": "^1.5.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "node-fetch": "^3.3.2"
  },
  "build": {
    "command": "npm run build",
    "publish": "dist"
  },
  "jobs": {
    "build-deploy": {
      "runs-on": "ubuntu-latest",
      "steps": [
        {
          "uses": "actions/checkout@v3"
        },
        {
          "name": "Install dependencies",
          "run": "npm ci"
        },
        {
          "name": "Build",
          "run": "npm run build"
        },
        {
          "name": "Deploy",
          "run": "echo \"Your deploy command here\""
        }
      ]
    }
  }
}
